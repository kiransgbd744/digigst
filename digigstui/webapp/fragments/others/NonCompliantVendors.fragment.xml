			<f:DynamicPageHeader pinnable="false">
				<fb:FilterBar showFilterConfiguration="false" useToolbar="false" class="sapUiNoContentPadding" width="90%" showGoOnFB="false"
					showClearOnFB="false">
					<fb:filterGroupItems>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="C" label="Financial Year" visibleInFilterBar="true">
							<fb:control>
								<Select id="idFiYear" forceSelection="true" items="{oFyModel>/finYears}">
									<core:Item key="{oFyModel>fy}" text="{oFyModel>fy}"/>
								</Select>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="B" label="Return Type" visibleInFilterBar="true">
							<fb:control>
								<Select id="idRetType" change="onReturnTypeChange" forceSelection="true">
									<core:Item key="GSTR1" text="GSTR-1"/>
									<core:Item key="GSTR3B" text="GSTR-3B"/>
									<core:Item key="CMP08" text="CMP08"/>
									<core:Item key="GSTR4" text="GSTR-4"/>
									<core:Item key="GSTR5" text="GSTR-5"/>
									<core:Item key="GSTR6" text="GSTR-6"/>
									<core:Item key="GSTR7" text="GSTR-7"/>
									<core:Item key="GSTR8" text="GSTR-8"/>
									<core:Item key="GSTR9" text="GSTR-9"/>
									<core:Item key="GSTR9A" text="GSTR-9A"/>
									<core:Item key="ITC04" text="ITC04"/>
								</Select>
							</fb:control>
							<!--CMP08 GSTR1 GSTR3B GSTR4 GSTR5 GSTR6 GSTR7 GSTR8 GSTR9 GSTR9A ITC04-->
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="Z" label="View Type" visibleInFilterBar="true">
							<fb:control>
								<Select id="idViewType" forceSelection="true">
									<core:Item key="ALL" text="All"/>
									<core:Item key="Compliant" text="Only Compliant"/>
									<core:Item key="NonCompliant" text="Not Compliant"/>
								</Select>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="E" label="Vendor PAN" visibleInFilterBar="true">
							<fb:control>
								<MultiInput id="idVenPan" showSuggestion="true" showValueHelp="true" valueHelpRequest="onHelpVendorPan" tokens="{Token>/}">
									<tokens>
										<Token key="{Token>pan}" text="{Token>pan}"/>
