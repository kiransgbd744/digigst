			<f:DynamicPageHeader class="sapUiNoContentPadding" pinnable="false">
				<fb:FilterBar showFilterConfiguration="false" useToolbar="false" width="90%" class="sapUiNoContentPadding" showGoOnFB="false"
					showClearOnFB="false">
					<fb:filterGroupItems>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="B" label="{i18n>gstin}" visibleInFilterBar="true">
							<fb:control>
								<MultiComboBox id="id_SetOffGstin" items="{path:'userPermission>/respData/dataSecurity/gstin/'}" selectionChange="selectAll" width="100%">
									<core:Item key="{userPermission>value}" text="{userPermission>value}"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="C" label="{i18n>taxPeriod}" visibleInFilterBar="true">
							<fb:control>
								<DatePicker id="id_SetOFfTaxPeriod" displayFormat="MMM yyyy" valueFormat="MMyyyy"/>
							</fb:control>
						</fb:FilterGroupItem>
					</fb:filterGroupItems>
				</fb:FilterBar>
				<HBox justifyContent="End" class="clMain" width="10%">
					<Button text="{i18n>clear}" class="sapUiTinyMarginBeginEnd buttoncolorSec" press="onClearSetOff"/>
