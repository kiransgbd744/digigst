	<f:DynamicPage>
		<f:title>
			<f:DynamicPageTitle>
				<f:content/>
				<f:actions>
					<Button class="buttoncolorSec" text="Request for Approval"/>
					<Button class="buttoncolorSec" text="Preview" press="onPreviewSummary"/>
					<Button class="buttoncolorSec" text="Save to GSTN" press="onSaveSummaryGSTN"/>
					<Button class="buttoncolorSec" text="Delete from ASP" visible="false" press="onDeleteSummary"/>
					<Button class="buttoncolorSec" text="Delete from GSTN" visible="false" press="onDeleteSummary"/>
					<Button class="buttoncolorSec" text="Delete from ASP and GSTN" visible="false" press="onDeleteSummary"/>
				</f:actions>
			</f:DynamicPageTitle>
		</f:title>
		<f:header>
			<f:DynamicPageHeader class="sapUiNoContentPadding">
				<fb:FilterBar id="fbInSupply" showGoOnFB="true" showClearOnFB="true" showFilterConfiguration="false" search="onSearch" clear="onClearFilter"
					useToolbar="false" class="sapUiNoContentPadding">
					<fb:filterGroupItems>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="A" label="Entity" visibleInFilterBar="true">
							<fb:control>
								<MultiComboBox id="slInEntity" items="{EntityModel>/resp/}" selectionChange="onChangeEntity" width="100%">
									<core:Item key="{EntityModel>entityId}" text="{EntityModel>entityName}"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="B" label="Supplier GSTIN" visibleInFilterBar="true">
							<fb:control>
								<MultiComboBox id="slInGstin" items="{SuppGstinModel>/resp/}" width="100%">
									<core:Item key="{SuppGstinModel>gstin}" text="{SuppGstinModel>gstin}"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="C" label="Tax Period" visibleInFilterBar="true">
							<fb:control>
								<DateRangeSelection id="inDrsPeriod" displayFormat="MMM yyyy"/>
							</fb:control>
						</fb:FilterGroupItem>
					</fb:filterGroupItems>
				</fb:FilterBar>
			</f:DynamicPageHeader>
		</f:header>
