									<core:Item key="B2B" text="B2B"/>
									<core:Item key="SEZWP" text="SEZWP"/>
									<core:Item key="SEZWOP" text="SEZWOP"/>
									<core:Item key="DXP" text="DXP"/>
								</MultiComboBox>
							</fb:control>
