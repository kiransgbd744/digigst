									<core:Item key="DR" text="DR"/>
									<core:Item key="CR" text="CR"/>
									<core:Item key="INV - A" text="INV - A"/>
									<core:Item key="DR - A" text="DR - A"/>
									<core:Item key="CR - A" text="CR - A"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="H1" label="Type" visibleInFilterBar="true" id="id_TypeFilter1">
							<fb:control>
								<MultiComboBox id="detailTypeID" selectionChange="onSelectionTypeChange2">
									<core:Item key="All" text="All"/>
									<core:Item key="A2" text="A2" id="id_A21"/>
