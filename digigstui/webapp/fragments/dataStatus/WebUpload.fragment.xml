<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:u="sap.ui.unified">
	<u:FileUploader id="fileUploader" fileType="xlsx,csv,xls,xlsm" sameFilenameAllowed="false" multiple="false" sendXHR="true" name="file"
		useMultipart="true" style="Emphasized" uploadComplete="onUploadComplete" visible="false" width="50%"
		class="sapUiMediumMarginBegin sapUiSmallMarginTop sapUiLargeMarginBottom"/>
	<Button type="Emphasized" id="btnUpload" text="{i18n>upload}" press="onPressUpload" visible="false"
		class="sapUiTinyMarginBegin sapUiSmallMarginTop sapUiLargeMarginBottom"/>
</core:FragmentDefinition>