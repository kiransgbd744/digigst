<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Initiate get call" type="Standard" contentWidth="30rem" class="sapUiSizeCompact">
		<buttons>
			<Button text="Initiate" press="onCloseComplianceCall('I')" class="buttoncolorSec"/>
			<Button text="Cancel" press="onCloseComplianceCall('C')" class="buttoncolorSec"/>
		</buttons>
		<HBox renderType="Bare" class="sapUiResponsiveMargin">
			<Text text="Initiate get call for those vendors where Return Filing status is not fetched in last days"/>
			<Select id="slCompCallDays" class="sapUiSmallMarginBegin">
				<core:Item key="0" text="0"/>
				<core:Item key="5" text="5"/>
				<core:Item key="10" text="10"/>
				<core:Item key="15" text="15"/>
				<core:Item key="30" text="30"/>
			</Select>
		</HBox>
	</Dialog>
</core:FragmentDefinition>