<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:fb="sap.ui.comp.filterbar">
	<Dialog id="dGstnAdapt" title="Adapt Filters" type="Message" class="sapUiSizeCompact">
		<fb:FilterBar showGoOnFB="false" showClearOnFB="false" showFilterConfiguration="false" useToolbar="false" hSpacing="0" vSpacing="0">
			<fb:filterGroupItems>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="A" label="{i18n>profitCentre}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/profitCenter}">
					<fb:control>
						<MultiComboBox id="slGstnProfitCtr" items="{GstnSecurity>/profitCenter}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="B" label="{i18n>plantCode}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/plant}">
					<fb:control>
						<MultiComboBox id="slGstnPlant" items="{GstnSecurity>/plant}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="C" label="{i18n>division}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/division}">
					<fb:control>
						<MultiComboBox id="slGstnDivision" items="{GstnSecurity>/division}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="D" label="{i18n>location}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/location}">
					<fb:control>
						<MultiComboBox id="slGstnLocation" items="{GstnSecurity>/location}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="E" label="{i18n>salesOrg}" visibleInFilterBar="true"
					visible="{=${GstnSecurity>/items/salesOrg}&amp;&amp;${Properties>/outward}}">
					<fb:control>
						<MultiComboBox id="slGstnSalesOrg" items="{GstnSecurity>/salesOrg}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="F" label="{i18n>purcOrg}" visibleInFilterBar="true"
					visible="{=${GstnSecurity>/items/purchOrg}&amp;&amp;!${Properties>/outward}}">
					<fb:control>
						<MultiComboBox id="slGstnPurcOrg" items="{GstnSecurity>/purchOrg}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="G" label="{i18n>distrChannel}" visibleInFilterBar="true"
					visible="{=${GstnSecurity>/items/distChannel}&amp;&amp;${Properties>/outward}}">
					<fb:control>
						<MultiComboBox id="slGstnDistrChannel" items="{GstnSecurity>/distChannel}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="H" label="{i18n>userAccess}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess1}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess1" items="{GstnSecurity>/userAccess1}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="I" label="{i18n>userAccess2}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess2}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess2" items="{GstnSecurity>/userAccess2}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="J" label="{i18n>userAccess3}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess3}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess3" items="{GstnSecurity>/userAccess3}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="K" label="{i18n>userAccess4}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess4}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess4" items="{GstnSecurity>/userAccess4}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="L" label="{i18n>userAccess5}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess5}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess5" items="{GstnSecurity>/userAccess5}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="M" label="{i18n>userAccess6}" visibleInFilterBar="true"
					visible="{GstnSecurity>/items/userAccess6}">
					<fb:control>
						<MultiComboBox id="slGstnUserAccess6" items="{GstnSecurity>/userAccess6}">
							<core:Item key="{GstnSecurity>value}" text="{GstnSecurity>value}"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
			</fb:filterGroupItems>
		</fb:FilterBar>
		<beginButton>
			<Button id="bGstnApply" text="Apply" type="Default" press="onPressFilterClose"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" type="Default" press="onPressFilterClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>