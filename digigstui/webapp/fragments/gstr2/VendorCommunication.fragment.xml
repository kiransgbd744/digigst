								</HBox>
								<ui:template>
									<!--<HBox alignItems="Start" justifyContent="End">
									<core:Icon src="sap-icon://download" size="0.75rem" press="onDownloadFileUpld" tooltip="Download Error " class="sapUiTinyMarginBegin"
										visible="{= ${venUpldTabData>error} === 'Processed'}"/>
								</HBox>-->
									<HBox alignItems="Start" justifyContent="Center">
										<Text text="{venUpldTabData>error}"/>
										<!--<core:Icon src="sap-icon://download" size="0.75rem" press="onDownloadFileUpld" class="sapUiTinyMarginBegin"/>-->
										<!--<core:Icon src="sap-icon://download" size="0.75rem" visible="{= ${venUpldTabData>error} === 0 ? false:true}"
										tooltip="Download Error Catalogue" class="sapUiTinyMarginBegin"
										press="onPrsFileStatusErrDownload($event,${venUpldTabData>error},'processed','active')"/>-->
										<core:Icon src="sap-icon://download" size="0.75rem"
											visible="{= (${venUpldTabData>error} !== 0 &amp;&amp; ${userPermission>/appPermission/P8})}" tooltip="Download" class="sapUiTinyMarginBegin"
											press="onPrsFileStatusErrDownload"/>
										<!--visible="{parts:[{path:'venUpldTabData>error'},{path:'venUpldTabData>fileStatus'}], formatter : '.formatter.iconVisibleErr'}"-->
									</HBox>
								</ui:template>
							</ui:Column>
							<ui:Column hAlign="Center" width="">
								<HBox width="100%">
									<Label text="Informational Records" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
									<core:Icon src="sap-icon://download" size="0.75rem" press="onDownloadCatalog" tooltip="Download" class="sapUiTinyMarginBegin"
										visible="{=${userPermission>/appPermission/P8}}"/>
								</HBox>
								<ui:template>
									<HBox alignItems="Start" justifyContent="Center">
										<Text text="{venUpldTabData>information}"/>
										<core:Icon src="sap-icon://download" size="0.75rem"
											visible="{= (${venUpldTabData>information} !== 0 &amp;&amp; ${userPermission>/appPermission/P8})}" tooltip="Download"
											class="sapUiTinyMarginBegin" press="onPrsFileStatusInformDwnld"/>
										<!--visible="{parts:[{path:'venUpldTabData>information'},{path:'venUpldTabData>fileStatus'}], formatter : '.formatter.iconVisibleInfrm'}"-->
										<!--<core:Icon src="sap-icon://download" size="0.75rem" visible="{= ${venUpldTabData>information} === 0 ? false:true}"
										tooltip="Download Error Records" class="sapUiTinyMarginBegin"
										press="onPrsFileStatusInformDwnld($event,${venUpldTabData>information},'Processed','active')"/>-->
									</HBox>
								</ui:template>
							</ui:Column>
						</ui:columns>
						<ui:footer>
							<HBox width="100%" justifyContent="Center">
								<Button id="btnFirstVC" type="Ghost" icon="sap-icon://media-rewind" tooltip="{i18n>firstPage}" class="sapUiTinyMarginBeginEnd"
