								<Button id="btnLastVC" type="Ghost" icon="sap-icon://media-forward" tooltip="{i18n>lastPage}" class="sapUiTinyMarginBeginEnd"
									enabled="false" press="onPressPaginationVC"/>
							</HBox>
						</ui:footer>
					</ui:Table>
				</VBox>
