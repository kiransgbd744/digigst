								<Text id="txtPageNoVC" text="/ 0" class="sapUiTinyMarginBeginEnd sapUiTinyMarginTop"/>
								<Button id="btnNextVC" type="Ghost" icon="sap-icon://navigation-right-arrow" tooltip="{i18n>next}" enabled="false"
