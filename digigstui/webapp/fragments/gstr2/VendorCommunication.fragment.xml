									<core:Item key="{vendorNameData>vendorName}" text="{vendorNameData>vendorName}"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="J" label="Vendor Code" visibleInFilterBar="true">
