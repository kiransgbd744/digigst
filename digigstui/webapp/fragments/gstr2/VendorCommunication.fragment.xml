									press="onVenUpldDwndTempt1" enabled="{= ${userPermission>/appPermission/P8}}"/>
							</Toolbar>
						</ui:extension>
						<ui:columns>
							<ui:Column width="4rem" hAlign="Center">
								<Label text="Sr.No" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
								<ui:template>
									<Text text="{venUpldTabData>SrNumber}"/>
								</ui:template>
							</ui:Column>
							<ui:Column width="" hAlign="Center">
								<Label text="Uploaded by" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
								<ui:template>
									<Text text="{venUpldTabData>updatedBy}"/>
								</ui:template>
							</ui:Column>
							<ui:Column width="" hAlign="Center">
								<Label text="Date of Upload" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
								<ui:template>
									<Text text="{venUpldTabData>updatedOn}"/>
								</ui:template>
							</ui:Column>
							<ui:Column width="" hAlign="Center">
								<Label text="File Type" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
								<ui:template>
									<Text text="{venUpldTabData>fileType}"/>
								</ui:template>
							</ui:Column>
							<ui:Column width="18rem">
								<Label text="File Name" textAlign="Center" width="100%" wrapping="true" design="Bold"/>
