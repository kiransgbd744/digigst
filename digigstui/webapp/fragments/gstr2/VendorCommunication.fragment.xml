									<core:Item key="{vendorNameData>vendorName}" text="{vendorNameData>vendorName}"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>-->
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="I" label="Response Date" visibleInFilterBar="true">
							<fb:control>
								<DatePicker id="idVRdate" displayFormat="dd MMM yyyy" valueFormat="dd-MMM-yyyy"/>
							</fb:control>
						</fb:FilterGroupItem>
						<!--<fb:FilterGroupItem groupName="G15" groupTitle="Basic" name="Ik" label="Tax Period" visibleInFilterBar="true" mandatory="true">
							<fb:control>
								<DatePicker id="idEmailToDateR12" displayFormat="MMM yyyy" valueFormat="MMyyyy" width="12rem"/>
							</fb:control>
						</fb:FilterGroupItem>-->
						<fb:FilterGroupItem groupName="G1" groupTitle="Basic" name="G" label="Status" visibleInFilterBar="true">
							<fb:control>
								<MultiComboBox id="idReptTypeR" width="100%">
									<core:Item key="true" text="Read"/>
									<core:Item key="false" text="Unread"/>
								</MultiComboBox>
							</fb:control>
						</fb:FilterGroupItem>
					</fb:filterGroupItems>
				</fb:FilterBar>
				<HBox id="fbGenReptHboxR" justifyContent="Start" width="100%" visible="false">
					<Button id="bGenRept1" class="buttoncolor" text="Filter" press="onVendorResponseGo" enabled="{= ${userPermission>/appPermission/P8}}"/>
				</HBox>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<Panel class="sapFDynamicPageAlignContent">
				<VBox>
					<Toolbar height="3rem" id="toolId">
						<ToolbarSpacer/>
						<SegmentedButton id="idsbFileStatus11" selectedKey="FileUpload" selectionChange="onSelChngVComFileStatus1" width="20rem" visible="true">
							<items>
								<SegmentedButtonItem text="File Upload" key="FileUpload"/>
								<SegmentedButtonItem text="API Push" key="ApiPush"/>
							</items>
						</SegmentedButton>
					</Toolbar>
