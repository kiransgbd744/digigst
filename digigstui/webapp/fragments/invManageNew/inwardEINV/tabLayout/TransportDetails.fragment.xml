<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.f" xmlns:l="sap.ui.layout" xmlns:uxap="sap.uxap"
	xmlns:fb="sap.ui.comp.filterbar" xmlns:ui="sap.ui.table" xmlns:form="sap.ui.layout.form">
	<l:Grid defaultSpan="L2" hSpacing="2" vSpacing="2" class="sapUiTinyMarginTop">
		<VBox>
			<Label text="Transporter ID" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/transporterID}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Transporter Name" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/transporterName}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Transport Mode" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/transportMode}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Transport Doc No" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/transportDocNo}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Transport Doc Date" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/transportDocDate}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Distance" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/distance}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Vehicle No" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/vehicleNo}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Vehicletype" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/vehicleType}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="E-Way Bill Number" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/eWayBillNumber}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="E-Way Bill Date" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/eWayBillDate}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Valid Upto" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/validUpto}" class="sapUiTinyMarginBegin"/>
		</VBox>
	</l:Grid>
</core:FragmentDefinition>