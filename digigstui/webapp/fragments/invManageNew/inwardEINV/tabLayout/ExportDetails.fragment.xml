<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.f" xmlns:l="sap.ui.layout" xmlns:uxap="sap.uxap"
	xmlns:fb="sap.ui.comp.filterbar" xmlns:ui="sap.ui.table" xmlns:form="sap.ui.layout.form">
	<l:Grid defaultSpan="L2" hSpacing="2" vSpacing="2" class="sapUiTinyMarginTop">
		<VBox>
			<Label text="Currency Code" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/currencyCode}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="{i18n>countryCode}" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/countryCode}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="{i18n>portCode}" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/portCode}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="{i18n>shippingBillNo}" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/shippingBillNumber}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="{i18n>shippingBillDate}" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/shippingBillDate}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Claim Refund Flag" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/claimRefundFlag}" class="sapUiTinyMarginBegin"/>
		</VBox>
		<VBox>
			<Label text="Export Duty" class="sapUiTinyMarginBegin titleFontEinv" design="Bold"/>
			<Text text="{EInvMgmtDetail>/0/exportDuty}" class="sapUiTinyMarginBegin"/>
		</VBox>
	</l:Grid>
</core:FragmentDefinition>