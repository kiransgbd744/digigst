<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.f">
	<Dialog id="copyDataID" title="Do you want to Copy Data to ASP User Input?" type="Message" showHeader="true" contentWidth="30%"
		class="sapUiSizeCompact cl_saveHeader">
		<VBox>
			<RadioButtonGroup id="rbgCopyType" selectedIndex="0" columns="2">
				<RadioButton id="RBDigiCompId" text="As per DigiGST Processed Data"/>
				<RadioButton id="RBDigiGstnId" text="GSTN Auto Computed"/>
			</RadioButtonGroup>
		</VBox>
		<beginButton>
			<Button text="Submit" type="Emphasized" press="onDialogCopySubmit"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onCloseDialogCopy"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>